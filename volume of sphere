import 'dart:io';

void main(){
  const double PI = 3.14;
  double r,vos;
  
  // Prompt the user to enter a number
  print("Enter volume of sphere:");
  
  // Read user input
  String? input = stdin.readLineSync();
if(input != null){
    r = double.parse(input);
 vos = (4 / 3) * PI * r * r * r;

 // Display the number
  print('$vos');
}
else{
    print("Invalid ");
}
}
